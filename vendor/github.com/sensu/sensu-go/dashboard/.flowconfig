[version]
0.82.0

[include]
<PROJECT_ROOT>/node_modules

[ignore]
<PROJECT_ROOT>/node_modules/.*\.nyc_output.*\.json$

[lints]
all=error

[libs]
<PROJECT_ROOT>/flow-typed

[options]
emoji=true
include_warnings=true
suppress_comment=\\(.\\|\n\\)*\\$ExpectError
module.ignore_non_literal_requires=true

module.name_mapper='^raw-loader!.*$' -> '<PROJECT_ROOT>/flow-stub/rawFlowStub.js.flow'
module.name_mapper='^\($\|/.*$\)' -> '<PROJECT_ROOT>/src\1'
module.name_mapper='^@sensu/lib\($\|/.*$\)' -> '<PROJECT_ROOT>/lib\1'
